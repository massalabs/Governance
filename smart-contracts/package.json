{
    "name": "my-massa-sc",
    "version": "0.0.2",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "asp --summary",
        "build": "massa-as-compile",
        "clean": "rimraf build",
        "prettier": "prettier '**/src/**/*.ts' --check && as-prettier --check assembly",
        "prettier:fix": "prettier '**/src/**/*.ts' --write && as-prettier --write assembly",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "fmt:check": "npm run prettier && npm run lint",
        "fmt": "npm run prettier:fix && npm run lint:fix",
        "oracle:feed": "tsx src/oracle/feeder/index.ts",
        "oracle:upgrade-contract": "npm run build && tsx src/oracle/upgrade-contract.ts",
        "masog:withdraw-coins": "tsx src/masog/withdraw-coins.ts",
        "masog:upgrade-contract": "npm run build && tsx src/masog/upgrade-contract.ts",
        "governance:e2e": "tsx src/governance/e2e-test.ts",
        "governance:upgrade-contract": "npm run build && tsx src/governance/upgrade-contract.ts",
        "deploy:all": "npm run build && tsx src/deploy/deploy-all.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@as-pect/cli": "^8.1.0",
        "@assemblyscript/loader": "^0.27.29",
        "@massalabs/as-types": "^2.0.0",
        "@massalabs/eslint-config": "^0.0.11",
        "@massalabs/massa-as-sdk": "^3.0.1-dev",
        "@massalabs/massa-web3": "^5.2.1-dev",
        "@massalabs/massa-sc-compiler": "^0.2.1-dev",
        "@massalabs/massa-sc-deployer": "^1.3.0",
        "@massalabs/prettier-config-as": "^0.0.2",
        "@massalabs/sc-standards": "^1.3.0",
        "@types/node": "^18.11.10",
        "assemblyscript": "^0.27.29",
        "assemblyscript-prettier": "^1.0.7",
        "chalk": "^5.4.1",
        "dotenv": "^16.0.3",
        "prettier": "^2.8.1",
        "tslib": "^2.4.0",
        "tsx": "^4.19.4",
        "typescript": "^4.8.4"
    },
    "overrides": {
        "visitor-as": {
            "assemblyscript": "$assemblyscript"
        }
    },
    "type": "module",
    "prettier": "@massalabs/prettier-config-as",
    "engines": {
        "node": ">=16"
    },
    "dependencies": {}
}